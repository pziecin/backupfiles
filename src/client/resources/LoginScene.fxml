<Pane prefHeight="320.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controller.LoginController">
   <children>
      <TextField fx:id="usernameTextField" layoutX="183.0" layoutY="90.0" prefHeight="20.0" prefWidth="220.0" promptText="username" />
      <PasswordField fx:id="passwordField" layoutX="183.0" layoutY="123.0" prefHeight="20.0" prefWidth="220.0" promptText="password" />
      <TextField fx:id="socketTextField" layoutX="183.0" layoutY="156.0" prefHeight="25.0" prefWidth="220.0" promptText="socket eg 7123" />
      <Button fx:id="ButtonLogIn" layoutX="183.0" layoutY="185.0" mnemonicParsing="false" onAction="#handleButtonClick" prefWidth="100.0" style="-fx-background-color: #66ff99;" text="Log In" textOverrun="CLIP">
         <textFill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="BLACK" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="CancelButton" layoutX="303.0" layoutY="185.0" mnemonicParsing="false" onAction="#exit" prefWidth="100.0" style="-fx-background-color: #66ff99;" text="Cancel" />
      <Button graphicTextGap="0.0" layoutX="248.0" layoutY="249.0" minWidth="53.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="156.0" style="-fx-background-color: #66ff99;" text="Register" />
      <Label layoutX="188.0" layoutY="37.0" text="Backuper" textAlignment="CENTER">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label layoutX="94.0" layoutY="94.0" text="Username" />
      <Label layoutX="94.0" layoutY="127.0" text="Password" />
      <Label layoutX="94.0" layoutY="160.0" text="Socket" />
      <Label layoutX="80.0" layoutY="256.0" text="Create youre own acount" />
   </children>
</Pane>
